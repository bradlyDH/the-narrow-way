{
  "expo": {
    "name": "The Narrow Way",
    "slug": "the-narrow-way",
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "narrowway",
    "version": "0.1.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.bradlydh.thenarrowway"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "f452ef20-451a-4cb5-af9c-5fdee59bec32"
      }
    },
    "owner": "bradlydh"
  }
}
