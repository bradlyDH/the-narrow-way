{
  "expo": {
    "name": "The Narrow Way",
    "slug": "the-narrow-way",
    "platforms": ["ios", "android"],
    "scheme": "narrowway",
    "version": "0.1.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/data/*", "assets/db/*", "**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bradlydh.thenarrowway",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "icon": "./assets/icon.png",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.bradlydh.thenarrowway",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-asset", "expo-sqlite"],
    "extra": {
      "eas": {
        "projectId": "f452ef20-451a-4cb5-af9c-5fdee59bec32"
      }
    },
    "owner": "bradlydh"
  }
}
